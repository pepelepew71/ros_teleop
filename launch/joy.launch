<?xml version="1.0"?>

<launch>

  <node pkg="joy" type="joy_node" name="joy">
    <param name="autorepeat_rate" value="2.0" />
    <param name="deadzone" value="0.05" />
  </node>

  <node pkg="ros_teleop" type="teleop_joy.py" name="teleop_joy">
    <remap from="cmd_vel" to="/joy_vel" />
    <rosparam>
      vx_max: 0.2
      wz_max: 0.1
    </rosparam>
  </node>

</launch>
